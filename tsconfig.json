{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "noImplicitAny": true,
    "target": "ES2022",
    "lib": [
      "DOM",
      "ES2022"
    ],
    "strict": true,
    "moduleDetection": "auto",
    "outDir": "./dist"
  }
}
